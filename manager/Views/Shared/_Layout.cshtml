
<!doctype html>
<html lang="en" data-layout="vertical" data-sidebar-visibility="show" data-topbar="dark" data-sidebar="dark" data-sidebar-size="lg" data-sidebar-image="none" data-preloader="disable" data-layout-style="default" data-bs-theme="light" data-layout-width="fluid" data-layout-position="fixed">
<plasmo-csui id="plasmo-inspector"></plasmo-csui>
<head>
    <style data-merge-styles="true"></style>
    @Html.Partial("~/Views/Shared/_title_meta.cshtml")
    @RenderSection("styles", required: false)
    @Html.Partial("~/Views/Shared/_head_css.cshtml")
    @Html.Partial("~/Views/Shared/_vendor_scripts.cshtml")
</head>
<body>
    <!-- Begin page -->
    <div id="layout-wrapper">
        @Html.Partial("~/Views/Shared/_menu.cshtml")

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">
                    @Html.Partial("~/Views/Shared/_page_title.cshtml")

                    @RenderBody()
                </div>
            </div>

            @RenderSection("externalhtml", required: false)
            @Html.Partial("~/Views/Shared/_footer.cshtml")
        </div>

    </div>

    

    @RenderSection("scripts", required: false)
</body>
</html>